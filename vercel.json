{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "api/app.py": {
          "maxDuration": 300
        }
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
